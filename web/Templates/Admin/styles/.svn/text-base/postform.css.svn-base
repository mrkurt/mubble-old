        form.AddContent
        {
            border: 1px solid #36679e;
            margin-top: 10px;
            background-color: #f6f6f6;
            width: 400px;
            margin-left: auto;
            margin-right: auto;
        }
        form.AddContent h2
        {
            margin: 0px;
            background-image: url(../Images/tab_back.gif);
            background-position: left bottom;
            background-repeat: repeat-x;
            color: white;
            font-size: 100%;
            padding: 2px 3px 2px 3px;
        }
        form.AddContent p
        {
            padding: 3px;
            margin: 0px;
        }
        form.AddContent p input.TextBox
        {
            width: 270px;
            height: 16px;
            border: 1px solid #dddddd;
            font-size: 12px;
            line-height: 16px;
        }
        form.AddContent p select
        {
            height: 16px;
            border: 1px solid #dddddd;
            font-size: 12px;
        }
        form.AddContent p label
        {
            display: block;
            width: 100px;
            float: left;
            padding: 3px;
            font-weight: bold;
        }
        
        div#PostForm
        {
            position: absolute;
            top: 10px;
            width: 720px;
            background-color: #F6F6F6;
            border: 1px solid #cccccc;
            padding-top: 0px;
            padding-bottom: 5px;
        }        
        
        div#PostForm h2.FormTitle
        {
            font-size: 100%;
            padding: 5px;
            margin: 0px;
        }

        div#PostForm label.Title
        {
            padding-left: 3px;
            padding-top: 3px;
            margin-bottom: 3px;
            display: block;
            width: 60px;
            text-align: left;
            position: absolute;
            font-size: 11px;
            margin-top: 3px;
            font-weight: bold;
        }
        div#PostForm input.Title
        {
            margin-bottom: 3px;
            margin-top: 3px;
