using System;
using System.Collections.Generic;
using System.Text;

namespace Mubble.Config
{
    public class Administration : BaseSection<Administration>
    {
        private string templatePath = "~/Templates/Admin/";

        public string TemplatePath
        {
            get { return templatePath; }
            set { templatePath = value; }
        }

        private string templateFileName = "Default.master";

        public string TemplateFileName
        {
            get { return templateFileName; }
            set { templateFileName = value; }
        }

        private bool newStyle = false;

        public bool NewStyle
        {
            get { return newStyle; }
            set { newStyle = value; }
        }

    }
}
