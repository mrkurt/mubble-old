            border: 1px solid #dddddd;
        }
        div#PostForm textarea
        {
            width: 100%;
            height: 400px;
            border: 1px solid #cccccc;
            border-left-width: 0px;
            border-right-width: 0px;
        }
        div.Expando
        {
            border-top: 1px solid #cccccc;
            padding-top: 5px;
            padding-bottom: 0px;
        }
        div#AdvancedOptions
        {
            padding-top: 5px;
        }
        div.OptionsContainer p input.TextBox
        {
            font-size: 11px;
            height: 14px;
            border: 1px solid #cccccc;
            width: 50%;
        }
        div.Expando h2
        {
            font-size: 11px;
            font-weight: bold;
            margin: 0px;
            padding: 2px;
            padding-left: 15px;
            background-image: url(../Images/icon_collapsed.gif);
            background-repeat: no-repeat;
            background-position: left center;
            cursor: pointer;
            width: 200px;
            background-color: #F6F6F6;
            padding-bottom: 5px;
        }
        div.Expanded h2
        {
            background-image: url(../Images/icon_expanded.gif);          
        }
        div.Expando div.OptionsContainer, div.Expando ul, div.Expando table
        {
            padding: 0px;
            font-size: 11px;
            display: none;
            margin: 0px;
            padding: 5px;
            border-top: 1px solid #dddddd;
            border-bottom: 1px solid #eeeeee;
            background-color: #ffffff;
        }
        div#PageSelector table
        {
            margin: 0px;
            padding: 0px;
        }
        div#PageSelector table thead th
        {
            text-align: left;
            background-color: #efefef;   
            padding: 3px;
        }
        div#PageSelector table td.FlowRight
        {
            padding-top: 3px;
            padding-bottom: 3px;
            padding-left: 3px;
        }
        div#PageSelector table td.PageNumber
        {
            white-space: nowrap;
        }
        div#TagSelector ul
        {
            list-style-type: none;
